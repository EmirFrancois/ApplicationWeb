<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping>
	<class name="bean.Chat" table="chats">
		<id name="idChat" column="id_Chat">
		<generator class="native"></generator>
		</id>
		
		<property name="nomFrancais" column="nomFrancais"></property>
		<property name="nomJaponnais" column="nomJaponnais"></property>
		<property name="niveau" column="niveau"></property>
		<property name="image" column="image"></property>
		<property name="momento" column="momento"></property>
		<property name="description" column="Description"></property>

	</class>
</hibernate-mapping>